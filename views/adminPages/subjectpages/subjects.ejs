<h1 class="text-center bg-dark text-info my-3 p-4 
rounded w-40 mx-auto">All Subjects</h1>
<div class="d-flex flex-row-reverse">
    <a class="btn text-info btn-outline-dark justify-content-end" href="/admin/addSub" role="button">Add Subject</a>
    <div class="p-2"></div>
</div>
<div class="events-list my-3 text-center">
    <table class="table table-dark table-striped">
        <thead>
            <tr>
                <!-- <th>Id</th> -->
                <th>Subject Name</th>
                <th>Subject Code</th>
                <th>Subject Department</th>
                <th>Prerequisite</th>
                <th>Doctor</th>
                <th>actions</th>
            </tr>
        </thead>
        <% if(subjects.length> 0){%>
            <tbody>
                <%for(let sub of subjects){%>
                    <tr>
                        <!-- <th>
                            <%= sub.id%>
                        </th> -->
                        <th>
                            <%= sub.SUB_name%>
                        </th>
                        <th>
                            <%= sub.SUB_code%>
                        </th>
                        <th>
                            <%= sub.SUB_depart%>
                        </th>
                        <th>
                            <%= sub.SUB_prev%>
                        </th>
                        <th>
                            <%= sub.SUB_doctor%>
                        </th>
                        <th>
                            <a href="/admin/<%=sub.id%>/editSub" class="mx-1 btn btn-success">update</a>
                            <a href="/admin/<%=sub.id%>/deleteSub" class="mx-1 btn btn-danger">delete</a>
                        </th>
                    </tr>
                    <%}%>
            </tbody>
            <%}%>
    </table>
</div>